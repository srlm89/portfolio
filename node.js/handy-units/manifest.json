{
  "manifest_version": 2,
  "name": "Unit Converter",
  "description": "wazoo",
  "version": "1.0",
  "homepage_url": "https://github.com/",
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "src/units.js",
      "src/parse-number.js",
      "src/parse-unit.js",
      "src/generate-conversions.js",
      "src/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "src/content-script.js"
      ]
    }
  ],
  "icons": {
    "16": "icons/16x16.png",
    "48": "icons/48x48.png",
    "96": "icons/96x96.png"
  }
}
